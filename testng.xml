<suite name="CloudStorageTests" parallel="classes" thread-count="4">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <test name="GCloud CLI Tests">
        <classes>
            <class name="com.cloudstorage.tests.GCloudStorageTests"/>
        </classes>
    </test>
    <test name="Playwright Tests">
        <classes>
            <class name="com.cloudstorage.tests.PlaywrightSignedURLTest"/>
        </classes>
    </test>
</suite>
